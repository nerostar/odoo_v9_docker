t[s[i]]=i;},date_part:function(date){var old=new Date(date);date.setHours(0);this.hour_start(date);if(date.getHours()&&(date.getDate()<old.getDate()||date.getMonth()<old.getMonth()||date.getFullYear()<old.getFullYear()))
