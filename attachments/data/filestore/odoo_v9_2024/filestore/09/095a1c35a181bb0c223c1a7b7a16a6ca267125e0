onLoad=null;t=null;},1);}};}
if(method=="GET"&&!this.cache){url+=(url.indexOf("?")>=0?"&":"?")+"dhxr"+new Date().getTime()+"=1";}
t.open(method,url,async);if(headers){for(var key in headers)
