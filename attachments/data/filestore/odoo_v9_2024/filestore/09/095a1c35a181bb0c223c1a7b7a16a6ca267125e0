Tour.log("Tour '"+state.id+"' is running",true);Tour.registerSteps(state.tour,state.mode);Tour.nextStep();}else{if(state.mode==="test"&&state.wait>=10){return Tour.error(state.step,"Tour '"+state.id+"' undefined");}
